ine":62,"column":18},"source":"src/scripts/hidden-word.js","original":{"line":41,"column":20}},{"generated":{"line":62,"column":30},"source":"src/scripts/hidden-word.js","original":{"line":41,"column":8}},{"generated":{"line":63,"column":0},"source":"src/scripts/hidden-word.js","original":{"line":42,"column":5}}],"sources":{"src/scripts/hidden-word.js":"export default class HiddenWord {\r\n    constructor(words) {\r\n        this.words = words;\r\n        this.currentIndex = 0;\r\n        this.container = document.getElementById(\"hidden-word\");\r\n\r\n        const prevBtn = document.getElementById(\"prev\");\r\n        const nextBtn = document.getElementById(\"next\");\r\n        prevBtn.addEventListener(\"click\", (e) => this.prev());\r\n        nextBtn.addEventListener(\"click\", (e) => this.next());\r\n    }\r\n\r\n    render(index = 0) {\r\n        const textArr = this.words[index].split(\"\");\r\n        this.container.innerHTML = \"\";\r\n\r\n        textArr.forEach((letter) => {\r\n            const card = document.createElement(\"div\");\r\n            card.className = \"flip-card\";\r\n            card.innerHTML = `<div class=\"flip-card-inner\">\r\n\t\t<div class=\"flip-card-front\"></div>\r\n\t\t\t\t<div class=\"flip-card-back\">${letter}</div>\r\n\t\t\t\t</div>\r\n\t\t\t`;\r\n\r\n            this.container.appendChild(card);\r\n        });\r\n    }\r\n\r\n    prev() {\r\n        const nextIndex = this.currentIndex - 1;\r\n        const currentIndex = Math.max(nextIndex, 0);\r\n        this.render(currentIndex);\r\n    }\r\n\r\n    next() {\r\n        const nextIndex = this.currentIndex + 1;\r\n        const lastIndex = this.words.length - 1;\r\n        const currentIndex = Math.min(nextIndex, lastIndex);\r\n\r\n        this.render(currentIndex);\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"403eb4d9b852508adc158b6a72bdb391","cacheData":{"env":{}}}