getStorageWords = () => localStorage.getItem(\"answers\");\r\n\r\nexport default function Settings() {\r\n    const storageWords = getStorageWords();\r\n    const [answers, setAnswers] = useState(storageWords || ANSWERS.join(\" \"));\r\n    const [loading, setLoading] = useState(false);\r\n\r\n    const onSubmit = () => {\r\n        setLoading(true);\r\n        setTimeout(() => {\r\n            localStorage.setItem(\"answers\", answers);\r\n            setLoading(false);\r\n        }, 1000);\r\n    };\r\n\r\n    const onClear = () => {\r\n        localStorage.clear();\r\n        setAnswers(ANSWERS.join(\" \"));\r\n    };\r\n\r\n    useEffect(() => {\r\n        return () => {\r\n            setAnswers(\"\");\r\n        };\r\n    }, []);\r\n\r\n    return (\r\n        <div className=\"container\">\r\n            <div className=\"row\">\r\n                <br />\r\n                <Link to=\"/\">\r\n                    <a className=\"button\">Назад к игру</a>\r\n                </Link>\r\n                <br />\r\n                <br />\r\n                <div className=\"column small-12\">\r\n                    <h2>Добавьте ответы с пробелом</h2>\r\n\r\n                    <textarea\r\n                        value={answers}\r\n                        onChange={(e) => setAnswers(e.target.value)}\r\n                        className=\"js-words\"\r\n                        name=\"words\"\r\n                        rows=\"3\"\r\n                    ></textarea>\r\n                    <div className=\"button-group align-justify\">\r\n                        <a\r\n                            className={`button expanded primary ${loading ? \"disabled\" : \"\"}`}\r\n                            onClick={onSubmit}\r\n                        >\r\n                            {loading ? \"Подождите...\" : \"Сохранить\"}\r\n                        </a>\r\n                        <a className=\"button expanded alert\" onClick={onClear}>\r\n                            Ответы по умолчанию\r\n                        </a>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"981d459c5be19afe38b028db83b198d3","cacheData":{"env":{}}}